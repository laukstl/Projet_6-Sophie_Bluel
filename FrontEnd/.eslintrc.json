{
    "plugins": [
        "@stylistic/js"
    ],
    
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "semi": ["error", "always"],
        "quotes": "off"
        // "camelcase": "off"
    }
}
